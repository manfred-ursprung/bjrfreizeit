<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Function</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <TCEforms>
                            <label>Select function</label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">Quick Search</numIndex>
                                        <numIndex index="1">Search->quickSearch;Search->searchResult</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Search extended</numIndex>
                                        <numIndex index="1">Search->extendedSearch;Search->searchResult</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">Suche</numIndex>
                                        <numIndex index="1">Search->search;Search->searchResult</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">Suchergebnis anzeigen</numIndex>
                                        <numIndex index="1">Search->searchResult</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </switchableControllerActions>
                    <settings.storagePage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Seiten mit Freizeiten</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>5</size>
                                <maxitems>5</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>

                            </config>
                        </TCEforms>
                    </settings.storagePage>

                </el>
            </ROOT>
        </sDEF>


    </sheets>
</T3DataStructure>