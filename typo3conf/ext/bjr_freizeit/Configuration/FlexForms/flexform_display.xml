<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Function</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>Select function</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">Display Items</numIndex>
										<numIndex index="1">Leisure->list</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">Search Items</numIndex>
										<numIndex index="1">Leisure->search</numIndex>
									</numIndex>
									<numIndex index="3">
										<numIndex index="0">Freizeiten im Detail darstellen</numIndex>
										<numIndex index="1">Leisure->show;Leisure->list</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>
					<settings.storagePage>
						<TCEforms>
							<exclude>1</exclude>
							<label>Seiten mit Artikel</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>5</size>
								<maxitems>5</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>

							</config>
						</TCEforms>
					</settings.storagePage>
				</el>
			</ROOT>
		</sDEF>
		<sListen>
			<ROOT>
				<TCEforms>
					<sheetTitle>Einstellungen f端r Listendarstellung</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.sorting>
						<TCEforms>
							<exclude>1</exclude>
							<label>Sortierung</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">alfabetisch</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Nach Einstelldatum</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Nach Ferienzeit</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>

								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.sorting>
					<settings.detailPage>
						<TCEforms>
							<exclude>1</exclude>
							<label>Detailseite zu der verlinkt wird</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>

							</config>
						</TCEforms>
					</settings.detailPage>
				</el>
			</ROOT>
		</sListen>
		<sDetailseite>
			<ROOT>
				<TCEforms>
					<sheetTitle>Einstellungen f端r Detailseite</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.listPage>
						<TCEforms>
							<exclude>1</exclude>
							<label>Seite f端r R端cksprung</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>

							</config>
						</TCEforms>
					</settings.listPage>
				</el>
			</ROOT>
		</sDetailseite>
	</sheets>
</T3DataStructure>